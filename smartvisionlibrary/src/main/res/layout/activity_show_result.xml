<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app = "http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#f2f2f2"
    android:layout_gravity="center_horizontal"
    android:id="@+id/result_root_view"
    >
    <LinearLayout
        android:id="@+id/result_vin_re"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:orientation="vertical"
        >
        <ImageView
            android:padding="5dp"
            android:id="@+id/result_image"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity = "center"
            />

        <com.kernal.smartvision.view.ResultLayout
            android:id="@+id/result_phone_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone"
            app:box_draw_type="line"
            app:draw_txt_size="20sp"
            app:interval_width="12dp"
            app:result_input_type="circle"
            app:item_height="20dp"
            app:item_width="15dp"
            app:pass_leng="phone"
            app:result_type="phone"
            />

        <com.kernal.smartvision.view.ResultLayout
            android:id="@+id/result_vin_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone"
            android:layout_weight="1"
            app:box_draw_type="line"
            app:draw_txt_size="13sp"
            app:interval_width="2dp"
            app:result_input_type="circle"
            app:item_height="22dp"
            app:item_width="18dp"
            app:pass_leng="vin"
            app:result_type="vin"
            />

    </LinearLayout>


    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">
        <RelativeLayout
            android:id="@+id/result_top_re"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#036eb8">
            <ImageButton
                android:id="@+id/result_back"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@drawable/btn_back" />

            <TextView
                android:id="@+id/result_title"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:textColor="#ffffff"
                android:textSize="18dip"
                />

            <TextView
                android:id="@+id/result_tv_feedback"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/result_feed_back"
                android:textColor="@color/tv_feed_back"
                android:textSize="15sp"
                />
        </RelativeLayout>
    </FrameLayout>


    <ListView
        android:id="@+id/result_vin_list"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scrollbars="none"
        android:divider="#00000000"
        android:visibility="gone"/>


    <ImageButton
        android:id="@+id/result_ok"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/btn_submit" />


 <!--   <ImageButton
        android:id="@+id/result_feedback"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/btn_feed" />-->

</RelativeLayout>