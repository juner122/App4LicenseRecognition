<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr format="reference" name="LoadingStyle"/>
    <color name="alivc_333">#333</color>
    <color name="alivc_black_alpha_eight">#CC000000</color>
    <color name="alivc_dialog_bg_translucent">#E6373d41</color>
    <color name="alivc_line_gray">#D7D8D9</color>
    <color name="alivc_text_cancel">#666</color>
    <color name="alivc_text_confirm">#00C1DE</color>
    <color name="alivc_white">#e7e7e7</color>
    <color name="colorPrimary_alpha_five">#1affffff</color>
    <declare-styleable name="LoadingView">
        <attr format="dimension" name="loading_view_size"/>
        <!--<attr name="android:color"/>-->
    </declare-styleable>
    <dimen name="alert_dialog_h">178dp</dimen>
    <dimen name="alert_dialog_w">300dp</dimen>
    <string name="app_name">AliyunVideoCommon</string>
    <string name="loading">正在加载</string>
    <string name="no_camera_permission">Camera usage is not permitted, please open first</string>
    <string name="no_read_external_storage_permission">External storage is not permitted to read, please open first</string>
